<?xml version="1.0"?>

<labwc_config>
  <desktops number="4"/>
  <margin top="0" bottom="0" left="0" right="0"/>

  <core>
    <gap>0</gap>
  </core>

  <theme>
    <name></name>
    <cornerRadius>1</cornerRadius>
    <font><name>sans</name><size>10</size></font>
  </theme>

  <keyboard>
    <default />
    <repeatRate>40</repeatRate>
    <repeatDelay>200</repeatDelay>
    <keybind key="W-r">
      <action name="Execute"><command>rofi -show run</command></action>
    </keybind>
  </keyboard>

  <snapping>
    <range>0</range>
    <topMaximize>no</topMaximize>
  </snapping>

  <resistance>
    <screenEdgeStrength>32</screenEdgeStrength>
  </resistance>

  <resize>
    <popupShow>Always</popupShow>
    <minimumArea>4</minimumArea>
  </resize>

</labwc_config>
