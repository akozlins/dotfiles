[org/gnome/evince/default]
continuous=true
dual-page=false
fullscreen=false
show-sidebar=false
sizing-mode='fit-width'

[org/gnome/gedit/plugins]
active-plugins=['modelines', 'filebrowser', 'quickhighlight', 'drawspaces', 'spell']

[org/gnome/gedit/plugins/drawspaces]
draw-spaces=['space', 'tab', 'leading', 'text', 'trailing']

[org/gnome/gedit/plugins/filebrowser]
root='file:///'
tree-view=true

[org/gnome/gedit/plugins/spell]
highlight-misspelled=true

[org/gnome/gedit/preferences/editor]
auto-indent=true
background-pattern='none'
create-backup-copy=false
display-line-numbers=true
display-overview-map=false
display-right-margin=true
editor-font='Monospace 12'
ensure-trailing-newline=false
highlight-current-line=true
insert-spaces=true
scheme='kate'
tabs-size=uint32 4
use-default-font=false
wrap-mode='none'

[org/gnome/gedit/preferences/ui]
notebook-show-tabs-mode='always'
show-tabs-mode='auto'
side-panel-visible=true
statusbar-visible=true

[org/gnome/meld]
draw-spaces=['space', 'tab', 'newline', 'nbsp', 'leading', 'text', 'trailing']
folder-ignore-symlinks=true
folder-time-resolution=1
highlight-syntax=true
ignore-blank-lines=false
indent-width=4
insert-spaces-instead-of-tabs=true
show-line-numbers=false
vc-commit-margin=80
vc-merge-file-order='local-merge-remote'
wrap-mode='none'

[org/gnome/terminal/legacy]
default-show-menubar=false

[org/gnome/terminal/legacy/profiles:/:default]
audible-bell=false
default-size-columns=110
default-size-rows=30
font='Misc Fixed Bold 12'
scrollbar-policy='never'
use-system-font=false
use-theme-colors=false
visible-name='default'

[org/gtk/gtk4/settings/file-chooser]
date-format='regular'
location-mode='path-bar'
show-hidden=true
show-size-column=true
show-type-column=true
sort-column='name'
sort-directories-first=true
sort-order='ascending'
type-format='category'

[org/gtk/settings/file-chooser]
date-format='regular'
location-mode='path-bar'
show-hidden=true
show-size-column=true
show-type-column=true
sort-column='name'
sort-directories-first=true
sort-order='ascending'
type-format='category'
