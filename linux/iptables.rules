*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:DOCKER-USER - [0:0]

# mark docker path
-I DOCKER-USER 1 -j MARK --set-mark 0xd0cca5e
# mark bridge path
-I DOCKER-USER 2 -m physdev --physdev-is-bridged -j MARK --set-mark 0x10ca1

# reset docker mark
-A FORWARD -m mark --mark 0xd0cca5e -j MARK --set-mark 0
-A FORWARD -j ACCEPT

COMMIT
