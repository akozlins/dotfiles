#!/bin/awk -f

BEGIN {
    t0 = ""; x0 = "";
    t1 = ""; x1 = "";
}

/^[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}/ {
    t = $1 "T" $2 "+00:00";
    x = $6 / 1;
    l0 = $0;

    $1 = $2 = "";
    if(x != x0 || x0 != x1) print t0 " " x0;
    t0 = t; x0 = x;
    t1 = t0; x1 = x0;
}

/^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}/ {
    t = $1;
    x = $5 / 1;

    if(x != x0 || x0 != x1) print t0 " " x0;
    t0 = t; x0 = x;
    t1 = t0; x1 = x0;
}
