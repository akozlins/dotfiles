#!/bin/sh
set -euf -o pipefail

export DVT_LICENSE_FILE=

export SWT_GTK3=0

exec \
"$HOME/downloads/eclipse/eclipse" \
-configuration "$XDG_CONFIG_HOME/eclipse/config" \
-data "$XDG_CONFIG_HOME/eclipse/data" \
--launcher.GTK_version 2 \
-vmargs -Duser.home=$XDG_CONFIG_HOME/eclipse \
&> /dev/null &
