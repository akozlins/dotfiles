#!/bin/sh
set -euf

confno=$1
uname=$ZOOM_UNAME
uid=$ZOOM_UID

if [ $# -eq 1 ] ; then
    exec \
    zoom "zoommtg://zoom.us/join?action=join&confno=$confno&uname=$uname&uid=$uid"
else
    pwd=$2
    exec \
    zoom "zoommtg://zoom.us/join?action=join&confno=$confno&uname=$uname&pwd=$pwd&uid=$uid"
fi
