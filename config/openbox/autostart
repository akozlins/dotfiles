( /usr/bin/xsetroot -solid black &> /dev/null & ) &

( sleep 1s; /usr/bin/fbpanel &> /dev/null & ) &
( sleep 1s; /usr/bin/uxterm &> /dev/null & ) &

( sleep 2s; /usr/bin/qxkb &> /dev/null &) &
( sleep 2s; /usr/bin/xset r rate 250 25 &> /dev/null & ) &

( sleep 5s; /usr/bin/xscreensaver -no-splash &> /dev/null & ) &
( sleep 5s; /usr/bin/xss-lock -- xscreensaver-command -lock &> /dev/null & ) &

( sleep 3s; /usr/bin/conky &> /dev/null & ) &
( sleep 3s; /usr/bin/pasystray &> /dev/null & ) &
