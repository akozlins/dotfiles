( hsetroot -solid "#000000" ) &

( sleep 1s; fbpanel &> /dev/null ) &
( sleep 1s; uxterm ) &

( sleep 5s; qxkb &> /dev/null ) &
( sleep 5s; xset r rate 250 25 ) &

( sleep 5s; xscreensaver -no-splash ) &
( sleep 5s; xss-lock -- xscreensaver-command -lock ) &

( sleep 5s; conky &> /dev/null ) &
( sleep 5s; pasystray &> /dev/null ) &
